<%= f.input :id, label: "Código", readonly: true%>

<%= s.input :fecharec, 
  input_html: { "data-behaviour" => "datepicker" },
  as: :string, 
  label: "Fecha Primera Recepción" 
%>

<%= s.association :usuario,
  collection: Usuario.all.order(:nombre),
  include_blank: false,
  label: "Asesor SJR",
  label_method: :nusuario, 
  value_method: :id 
%>

<%= s.association :oficina,
  collection: Sip::Oficina.habilitados,
  include_blank: false,
  label: "Oficina",
  label_method: :nombre, 
  value_method: :id 
%>

<%= f.input :fecha, 
  input_html: { "data-behaviour" => "datepicker" },
  as: :string, 
  label: "F. Desplazamiento Emblemático",
  wrapper_html: { style: "padding-bottom: 18px;" }
%>

<%= f.input :hora, :as => :string, 
  label: "Hora del Desplazamiento"
%>

