<h2>Municipios Expulsores</h2>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= Sip::Ubicacion.human_attribute_name(:id_pais) %></th>
      <th><%= Sip::Ubicacion.human_attribute_name(:id_departamento) %></th>
      <th><%= Sip::Ubicacion.human_attribute_name(:id_municipio) %></th>
      <th>Contactos</th>
      <th>Beneficiarios</th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    <% @expulsores.try(:each) do |cmun| %>
      <tr>
        <td><%= cmun['pais'] %></td>
        <td><%= cmun['departamento'] %></td>
        <td><%= cmun['municipio'] %></td>
        <td><%= cmun['contacto'] %></td>
        <td><%= cmun['beneficiario'] %></td>
        <td><%= cmun['npersona'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>


