<% if @params[:action] == "agregar" %>
  <% des_id = @params[:caso_acto_desplazamiento_id] %>
<% end %>
<% if @params[:action] == "eliminar" %>
  <% des_id = @params[:desplazamiento_id]%>
<% end %>
  <% byebug %>
<% if des_id == "" %>
  $("#actos_tabla").html("<%= escape_javascript(render('sivel2_sjr/casos/antecedentes')) %>");
<% else %>
  <% f_exp = Sivel2Sjr::Desplazamiento.find(des_id).fechaexpulsion %>
  $("#actos_<%= des_id %>").html("<%= escape_javascript(render('actos_tabla', f_exp: f_exp)) %>");
<% end %>
